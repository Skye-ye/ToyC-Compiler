int add(int a, int b) {
    return a + b;
}

int multiply(int x, int y) {
    int result = 0;
    int i = 0;
    while (i < y) {
        result = add(result, x);
        i = i + 1;
    }
    return result;
}

int main() {
    int x = 3;
    int y = 4;
    int product = multiply(x, y);
    return product;
}