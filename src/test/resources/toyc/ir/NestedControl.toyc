int main() {
    int x = 0;
    int y = 0;
    int max = 5;
    
    while (x < max) {
        if (x % 2 == 0) {
            y = y + x;
            if (y > 10) {
                y = y - 5;
            } else {
                y = y + 1;
            }
        } else {
            if (x > 2) {
                y = y * 2;
            }
        }
        x = x + 1;
    }
    
    return y;
}